fun fact(n: i64) -> i64 {
    if n == 0 {
        return 1;
    } else {
        return n * fact(n - 1);
    }
}

fun fib(n: i64) -> i64 {
    if n == 0 {
        return 0;
    } else {
        if n == 1 {
            return 1;
        } else {
            return fib(n - 1) + fib(n - 2);
        }
    }
}

fun b() {
    return 3 + 5 / (4 + 5);
}

fun a() {
    if b() == 3 {
        return 2;
    } else {
        return 3;
    }

    return b() + 2;
}

fun main() {
    return fact(if a() == 2 {
        return 5;
    } else {
        return 6;
    });
}
