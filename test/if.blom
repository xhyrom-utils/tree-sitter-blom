fun @native printf(format: string, ...) -> i32;

fun fact(n: i32) -> i32 {
    if n == 0 {
        return 1;
    } else {
        return n * fact(n - 1);
    }

    return 0;
}

fun fib(n: i32) -> i32 {
    if n == 0 {
        return 0;
    } else {
        if n == 1 {
            return 1;
        } else {
            return fib(n - 1) + fib(n - 2);
        }
    }

    return 0;
}

fun b() -> i32 {
    return 3 + 5 / (4 + 5);
}

fun a() -> i32 {
    if b() == 3 {
        printf("b is 3\n")
        return 2;
    } else {
        return 3;
    }

    return b() + 2;
}

fun main() -> i32 {
    i32 c = fact(a());

    printf("result: %d\n", c)

    return 0;
}
