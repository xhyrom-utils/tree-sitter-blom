fun fact(n: i64) -> i64 {
    if n == 0 {
        return 1;
    } else {
        return n * fact(n - 1);
    }
}

fun fib(n: i64) -> i64 {
    if n == 0 {
        return 0;
    } else {
        if n == 1 {
            return 1;
        } else {
            return fib(n - 1) + fib(n - 2);
        }
    }
}

fun main() {
    return fib(15);
}
