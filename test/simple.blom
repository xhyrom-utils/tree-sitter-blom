fun fib(n: i32) -> i32 {
    if n <= 1 {
        return n;
    }

    return fib(n - 1) + fib(n - 2);
}

fun main() -> i32 {
    i32 a = ~2 + 9;
    if a > 99 {
        return 9;
    }

    i32 i = 0;
    i32 a = 5;

    for a < 10; a = a + 1; {
        i = i + a;
    }

    for i32 g = 0; g < 10; g = g + 1; {
        i = i + g;
    }

    while i < 10 {
        i = i + 1;
    }

    return i;
}
